{
  "name": "vite-plugin-electron-renderer",
  "version": "0.10.3",
  "description": "Support use Node.js API in Electron-Renderer",
  "main": "plugins/index.mjs",
  "types": "plugins",
  "exports": {
    ".": {
      "import": "./plugins/index.mjs",
      "require": "./plugins/index.js"
    },
    "./plugins/*": {
      "import": "./plugins/*.mjs",
      "require": "./plugins/*.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electron-vite/vite-plugin-electron-renderer.git"
  },
  "author": "草鞋没号 <308487730@qq.com>",
  "license": "MIT",
  "scripts": {
    "dev": "node build.mjs --watch",
    "build": "tsc --noEmit && node build.mjs",
    "types": "tsc --emitDeclarationOnly",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "lib-esm": "~0.3.0"
  },
  "devDependencies": {
    "esbuild": "^0.15.13",
    "rollup": "^2.79.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.3",
    "vite-plugin-utils": "^0.3.6"
  },
  "files": [
    "plugins"
  ],
  "keywords": [
    "vite",
    "plugin",
    "electron",
    "renderer"
  ]
}
